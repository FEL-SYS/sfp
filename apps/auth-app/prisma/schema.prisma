generator client {
  provider = "prisma-client-js"
  output   = "../../../node_modules/@prisma/client-2"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model user {
  id       Int    @id(map: "PK_user_id") @default(autoincrement())
  username String @unique @db.VarChar
}
